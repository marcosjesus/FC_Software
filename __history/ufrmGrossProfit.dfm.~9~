      Precision = 38
      Size = 2
    end
  end
  object sqlExpense: TFDQuery
    Connection = DBDados.FDConnection
    SQL.Strings = (
      ''
      ''
      
        '     SELECT '#39'TAX COST OF GOODS'#39' AS DESCRIPTION, ID_PROCESS, -1 *' +
        ' TAX AS EXPENSE FROM TBPROCESS WHERE TABLENAME = '#39'TBINVOICE'#39' AND' +
        ' ID_PROCESS = 3'
      #9' UNION ALL'
      
        'SELECT '#39'MANUFACTURING OR RAW MATERIALS'#39', H.ID_PROCESS, SUM(I.TOT' +
        'ALAREA * P.COST_VALUE) * -1 AS PURCHASE_COST'
      'FROM TBPROCESS_ITEM I'
      
        'INNER JOIN TBPROCESS H ON H.ID_PROCESS = I.ID_PROCESS AND H.TABL' +
        'ENAME + '#39'_ITEM'#39' = I.TABLENAME'
      'INNER JOIN TBPRODUCT P ON P.ID_PRODUCT = I.ID_PRODUCT'
