  object sqlIncome: TFDQuery
    Connection = DBDados.FDConnection
    SQL.Strings = (
      
        #9'SELECT '#39'SALES REVENUE'#39' AS DESCRIPTION,  P.ID_PROCESS, (P.TOTAL ' +
        '- SUM(AMOUT)) AS SALES FROM TBPROCESS P'
      
        #9'LEFT OUTER JOIN TBSERVICE_ITEM I ON I.TABLENAME = '#39'TBINVOICE_IT' +
        'EM'#39' AND I.ID_PROCESS = 3'
      #9' WHERE P.ID_PROCESS = 3 AND P.TABLENAME = '#39'TBINVOICE'#39
      #9' GROUP BY P.ID_PROCESS, P.TOTAL'
      ''
      #9' UNION ALL'
      
        #9'SELECT '#39'LABOR SERVICE'#39' AS DESCRIPTION,  P.ID_PROCESS, SUM(AMOUT' +
        ') AS LABOR FROM TBPROCESS P'
      
        #9'LEFT OUTER JOIN TBSERVICE_ITEM I ON I.TABLENAME = '#39'TBINVOICE_IT' +
        'EM'#39' AND I.ID_PROCESS = 3'
      #9' WHERE P.ID_PROCESS = 3 AND P.TABLENAME = '#39'TBINVOICE'#39
      #9' GROUP BY P.ID_PROCESS, P.TOTAL')
    Left = 308
    Top = 72
    object sqlIncomeDESCRIPTION: TStringField
      FieldName = 'DESCRIPTION'
      Origin = 'DESCRIPTION'
      ReadOnly = True
      Required = True
      Size = 13
    end
    object sqlIncomeID_PROCESS: TIntegerField
      FieldName = 'ID_PROCESS'
      Origin = 'ID_PROCESS'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      ReadOnly = True
      Required = True
    end
