  object sqlProcess: TFDQuery
    Connection = DBDados.FDConnection
    SQL.Strings = (
      
        'SELECT A.ID_PROCESS, A.ID_COMPANY, A.ID_CUSTOMER, B.COMPANYNAME,' +
        ' A.DT_PROCESS, A.DT_PROCESS_VALID, A.ID_CONTRACTORS, C.NAME AS C' +
        'ONTRACTOR,'
      
        'A.CUSTOMER_NAME, A.CUSTOMER_PHONE, A.CUSTOMER_EMAIL, A.ADDRESS1,' +
        ' A.ZIPCODE, A.ST, A.CITY, A.COUNTY, A.PONUMBER,'
      
        'A.COMMENTS, A.SUBTOTAL, A.PERCENT_DISCOUNT, A.DISCOUNT, A.TAX, A' +
        '.SHIPPING, A.TOTAL, A.ADD_DATE, A.UPD_DATE, A.ID_USER,'
      'A.ID_ORIGEN, A.STATUS, A.DT_SHIPPING'
      'FROM TBPROCESS A With (NOLOCK)'
      'INNER JOIN TBCOMPANY B ON B.ID_COMPANY = A.ID_COMPANY'
      
        'INNER JOIN TBCONTRACTORS C ON C.ID_CONTRACTORS = A.ID_CONTRACTOR' +
        'S'
      'LEFT OUTER JOIN TBCONTRACTORS D ON D.ID_MAIN_USER = A.ID_USER'
      'where A.TABLENAME = :TABLENAME '
      '&WHERE1'
      '&WHERE2')
    Left = 964
    Top = 424
    ParamData = <
      item
        Name = 'TABLENAME'
        DataType = ftString
        ADDataType = dtAnsiString
        ParamType = ptInput
      end>
    MacroData = <
      item
        Value = Null
        Name = 'WHERE1'
        DataType = mdIdentifier
      end
      item
        Value = Null
        Name = 'WHERE2'
        DataType = mdIdentifier
      end>
