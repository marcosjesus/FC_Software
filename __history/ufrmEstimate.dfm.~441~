  object sqlProcessItem: TFDQuery
    Connection = DBDados.FDConnection
    SQL.Strings = (
      'SELECT I.ID_PROCESS_ITEM,'
      ' ID_PROCESS , '
      ' I.ID_PRICELIST, '
      ' I.ID_PRODUCT, '
      ' S.TRADININGNAME,'
      ' TP.DESCRIPTION,'
      ' TS.DESCRIPTION AS STYLE,'
      
        ' P.PRODUCT_STYLE,  P.PRODUCT_STYLE_NAME,  P.COLOR,  P.COLOR_NAME' +
        ','
      
        ' (P.PRODUCT_STYLE + '#39' '#39' +  P.PRODUCT_STYLE_NAME + '#39' '#39' +  P.COLOR' +
        '  + '#39' '#39' +  P.COLOR_NAME ) AS PRODUCT,'
      ' I.QTY, '
      ' I.RATE,'
      ' I.AMOUT,'
      ' P.TAXBLE,'
      ' I.ROOM,'
      ' I.WIDTH,'
      ' I.HEIGHT,'
      ' I.TOTALAREA,'
      ' I.REQ_PURCHASE_ORDER'
      ''
      'FROM TBPROCESS_ITEM I'
      
        'INNER JOIN TBPRODUCT P ON P.ID_PRODUCT = I.ID_PRODUCT AND P.TYPE' +
        'OFPRODUCT = '#39'PRODUCT'#39
      'INNER JOIN TBSUPPLIER S ON S.ID_SUPPLIER = P.ID_SUPPLIER'
      'INNER JOIN TBTYPEBRAND TP ON TP.ID_TYPEBRAND = P.ID_TYPE'
      'INNER JOIN TBTYPEBRAND TS ON TS.ID_TYPEBRAND = P.STYLE'
      'WHERE TABLENAME = :TABLENAME AND  ID_PROCESS = :ID_PROCESS'
      'UNION ALL'
      ''
      'SELECT I.ID_PROCESS_ITEM,'
      ' ID_PROCESS , '
      ' I.ID_PRICELIST, '
      ' I.ID_PRODUCT, '
      ' S.TRADININGNAME,'
      ' TP.DESCRIPTION,'
      ' TS.DESCRIPTION AS STYLE,'
      
        ' P.PRODUCT_STYLE,  P.PRODUCT_STYLE_NAME,  P.COLOR,  P.COLOR_NAME' +
        ','
      ' (P.PRODUCT_NAME ) AS PRODUCT,'
      ' I.QTY, '
      ' I.RATE,'
      ' I.AMOUT,'
      ' P.TAXBLE,'
      ' I.ROOM,'
      ' I.WIDTH,'
      ' I.HEIGHT,'
      ' I.TOTALAREA,'
      ' I.REQ_PURCHASE_ORDER'
      ''
      'FROM TBPROCESS_ITEM I'
      
        'INNER JOIN TBPRODUCT P ON P.ID_PRODUCT = I.ID_PRODUCT AND P.TYPE' +
        'OFPRODUCT = '#39'SERVICE'#39
      'INNER JOIN TBSUPPLIER S ON S.ID_SUPPLIER = P.ID_SUPPLIER'
      'LEFT OUTER JOIN TBTYPEBRAND TP ON TP.ID_TYPEBRAND = P.ID_TYPE'
      'LEFT OUTER JOIN TBTYPEBRAND TS ON TS.ID_TYPEBRAND = P.STYLE'
      'WHERE TABLENAME = :TABLENAME AND  ID_PROCESS = :ID_PROCESS')
    Left = 161
    Top = 598
    ParamData = <
      item
        Name = 'TABLENAME'
        DataType = ftString
        ADDataType = dtAnsiString
        ParamType = ptInput
      end
      item
        Name = 'ID_PROCESS'
        DataType = ftInteger
        ADDataType = dtInt32
        ParamType = ptInput
      end>
